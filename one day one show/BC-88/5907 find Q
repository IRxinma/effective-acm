#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
const int maxn = 1e5 + 10;
char s[maxn];
ll ans, res;
int main(){
    int t;
    scanf("%d", &t);
    while(t --){
        scanf("%s", s);
        ans=res=0;
        for(int i=0; s[i]; i++){
            if(s[i]=='q') res++;
            else res=0;
            ans+=res;
        }
        cout<<ans<<endl;
        //printf("%I64d\n", ans);
    }
    return 0;
}
