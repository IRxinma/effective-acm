#include<cstdio>
#include<iostream>
#include<algorithm>
#include<cstdlib>
#include<set>
#include<cmath>
#include<cstring>
#include<climits>
//给你n种操作数，问任意一种操作是否可以做到胜负参半
using namespace std;
int main(){
    int n, x;
    scanf("%d", &n);
    while(n--){
        scanf("%d", &x);
        printf("%s\n", x%2==0?"Bad":"Balanced");
    }
    return 0;
}
